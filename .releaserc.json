{
  "branches": "master",
  "repositoryUrl": "https://github.com/andrewdant-ert/semantic-test.git",
  "debug": "true",
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    [
      "@google/semantic-release-replace-plugin",
      {
        "replacements": [
          {
            "files": ["handheld/versionHH.txt"],
            "from": "\"HH_VERSION\": \".*\"",
            "to": "\"HH_VERSION\": \"${nextRelease.version}\"",
            "results": [
              {
                "file": "handheld/versionHH.txt",
                "hasChanged": true,
                "numMatches": 1,
                "numReplacements": 1
              }
            ],
            "countMatches": true
          },
          {
            "files": ["tablet/versionTB.txt"],
            "from": "\"TB_VERSION\": \".*\"",
            "to": "\"TB_VERSION\": \"${nextRelease.version}\"",
            "results": [
              {
                "file": "tablet/versionTB.txt",
                "hasChanged": true,
                "numMatches": 1,
                "numReplacements": 1
              }
            ],
            "countMatches": true
          }
        ]
      }
    ],
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "CHANGELOG.md"
      }
    ],
    [
      "@semantic-release/git",
      {
        "assets": ["package.json", "package-lock.json", "CHANGELOG.md", "handheld/*", "tablet/*"],
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
      }
    ]
  ]
}